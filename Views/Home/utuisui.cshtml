@{
    ViewBag.Title = "退水资料";
    Layout = "~/Views/Pages/Home.cshtml";
}
@section styles{
    <style>
        * {
            list-style: none;
        }

        ul {
            margin: 0;
            padding: 0;
            overflow: hidden;
        }

        .breadcrumb {
            margin-bottom: 10px;
        }

        .form-control {
            border-radius: 0;
        }

        .box1 {
            border: 1px solid #0094ff;
            overflow: hidden;
        }

            .box1 .m {
                border-bottom: 1px solid #aadcff;
                padding: 5px 0;
            }

            .box1 .ms {
                float: left;
                padding: 8px 10px;
                text-align: right;
            }

            .box1 .mms {
                width: 90px;
            }

            .box1 .s {
                float: left;
                padding: 2px;
            }

        .box2 {
            overflow: hidden;
        }

            .box2 .m {
                width: 50%;
                float: left;
                overflow: hidden;
                border: 1px solid #4fb5ff;
            }

            .box2 .mm {
                border-top: 1px solid #7fc4f6;
            }

                .box2 .mm li {
                    float: left;
                    padding: 3px 5px;
                }

        #form1 .form-control {
            width: 80px;
            height: 20px;
        }

        .box2 .A {
            width: 70px;
        }

        .box2 .B {
            width: 110px;
            text-align: center;
        }

        .box2 .C {
            width: 80px;
        }

        .box2 .D {
            width: 110px;
        }

        .box2 .E {
            overflow: hidden;
            float: none !important;
        }

        .box2 .title {
            background: #e1f3ff;
            text-align: center;
        }

            .box2 .title li {
                padding: 5px;
            }

        .subut {
            margin: 10px 0 10px 0;
            padding: 8px;
            border: 1px solid #0094ff;
        }

        #exit {
            margin-left: 10px;
        }

        .auto {
            margin: 10px 0 10px 0;
            border: 1px solid #0094ff;
            padding: 5px;
            overflow: hidden;
        }

            .auto .m {
                overflow: hidden;
            }

            .auto .sx {
                margin-top: 3px;
            }

            .auto .form-control {
                height: 26px;
                width: 80px;
                border-radius: 0;
            }

            .auto .btn {
                border-radius: 0;
                padding: 2px 12px;
            }

            .auto li {
                float: left;
                margin: 0 5px 0 5px;
            }

        .box2 li {
            text-align: center;
        }
    </style>
}
@section scripts{

}
<div class="box2">
    <div class="m">
        <ul class="title mm">
            <li class="A">名称</li>
            <li class="B">佣金</li>
            <li class="C">最小限额</li>
            <li class="D">单注限额</li>
            <li class="E">单项限额</li>
        </ul>
        @{
            var i = 1;
            foreach (var m in x9web.Api.WebClass.u_tuisui)
            {
                <ul class="mm">
                    <li class="A">@m.name.Trim()</li>
                    <li class="B">@decimal.Parse(m.yongjin.ToString()).ToString("0.###")</li>
                    <li class="C">@m.min_e</li>
                    <li class="D">@m.danzu_e</li>
                    <li class="E">@m.max_e</li>
                </ul>

                if (i % 27 == 0)
                {
                    if (i < 54)
                    {
                        @Html.Raw("</div>")
                        @Html.Raw("<div class='m' style='border-left:0'>")
                        <ul class="title mm">
                            <li class="A">名称</li>
                            <li class="B">佣金</li>
                            <li class="C">最小限额</li>
                            <li class="D">单注限额</li>
                            <li class="E">单项限额</li>
                        </ul>
                    }

                }
                i++;
            }
        }
    </div>
</div>