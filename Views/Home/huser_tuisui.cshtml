@section scripts{
    <script src="~/Scripts/apps/laydate.js"></script>
    <script>
        laydate.render({
            elem: ".select input[name='timediff']",
            range: ">>>"
        });
        $(".select #select_qisu").click(function () {
            var qisu = $(".select select[name='select_qisu']").val();
            var type = 1;
            var time = "";
            var index = layer.load(1, { shade: [0.3] });

            $.ajax({
                url: "/partial/manage_usertuisui",
                type: "POST",
                data: { id: qisu, type: type, time: time },
                success: function (data) {
                    layer.close(index);
                    $(".box").html(data);
                }

            })
        });
        $(".select button[name='select_time']").click(function () {
            var s = $(".select input[name='timediff']").val();
            if (s == "") {
                layer.alert("请选择需要查询的时间段！");
                return;
            }
            var type = 2;
            var time = s;
            var qisu = "";

            var index = layer.load(1, { shade: [0.3] });
            $.ajax({
                url: "/partial/manage_usertuisui",
                type: "POST",
                data: { id: qisu, type: type, time: time },
                success: function (data) {
                    layer.close(index);
                    $(".box").html(data);
                }
            })
        })

        function theList(obj) {
            var id = obj;
            var type = $(".box input[name='select_type']").val();
            var val = $(".box input[name='select_val']").val();

            var index = layer.load(1, { shade: [0.3] });
            $.ajax({
                url: "/partial/ustuisuizudan",
                type: "POST",
                data: { id: id, type: type, val: val },
                success: function (data) {
                    layer.close(index);
                    $(".box").html(data);
                }
            })
        }
    </script>
}
